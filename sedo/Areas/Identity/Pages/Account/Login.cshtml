<main class="content">
    <div id="content" class="row">
        <div id="loginForm">
            <div class="container">
                <div class="form">
                    <form method="post" id="fm1" action="login">
                        <h3 style="">
                            <i class="fas fa-id-card"></i>
                            <span class="form__headline">Enter your Username and Password</span>
                        </h3>

                        <section class="form-group form__input">
                        </section>

                        <section class="form-group form__input">

                            <label for="username" class="form__input-label"><span class="accesskey">U</span>sername:</label>
                            <div>
                                <input class="form-control required form__input-field" id="username" name="username" size="25" tabindex="1" type="text" value="" accesskey="u" autocomplete="off">
                            </div>
                        </section>

                        <section class="form-group form__input">
                            <label for="password" class="form__input-label"><span class="accesskey">P</span>assword:</label>

                            <div>
                                <input class="form-control required form__input-field" type="password" id="password" name="password" size="25" tabindex="2" accesskey="p" value="" autocomplete="off"><span id="capslock-on" style="display:none;">
                                    <i class="fa fa-exclamation-circle"></i>
                                    <span>CAPSLOCK key is turned on!</span>
                                </span>
                            </div>
                        </section>

                        <section class="form-group">
                        </section>

                        <input type="hidden" name="execution" value="72f5479f-01f6-44ec-b436-3e7a667ada7d_"><input type="hidden" name="service" value=""><input type="hidden" name="_eventId" value="submit"><input type="hidden" name="geolocation"><div class="form__row">
                            <button class="btn btn-block btn-submit" name="submit" accesskey="l" tabindex="6" type="submit" value="Log into my account">Log into my account</button>

                            <div class="form__registration-bar">
                                <p class="form__registration-hint">Don't have an account yet?</p>
                                <a asp-area="Identity" asp-page="/Account/Register" class="btn btn--unobtrusive">Register</a>
                            </div>
                        </div>


                    </form>

                    <div id="pmlinks">
                        <div>
                            <br><span class="fa fa-question-circle"></span>
                                <a asp-area="Identity" asp-page="/Account/Register" style="line-height:40px;">I forgot my password</a>
                        </div>
                    </div>

                    <aside>
                        <nav class="language-nav">
                            <ul class="language-list">
                                <li class="language-item">
                                    <a href="#">English (US)</a>
                                </li>
                                <li class="language-item">
                                    <a href="#">English (UK)</a>
                                </li>
                                <li class="language-item">
                                    <a href="#">Deutsch</a>
                                </li>
                                <li class="language-item">
                                    <a href="#">Español</a>
                                </li>
                                <li class="language-item">
                                    <a href="#">Français</a>
                                </li>
                                <li class="language-item">
                                    <a href="#">中文（简体)</a>
                                </li>
                                <li class="language-item">
                                    <a href="#">Português</a>
                                </li>
                            </ul>
                        </nav>
                    </aside>

                    <script>/*<![CDATA[*/
                        var i = "One moment please..."
                        var j = "Log into my account"
                            /*]]>*/
                            $(window).on('pageshow', function(){
                                $(':submit').prop('disabled', false);
                                $(':submit').attr('value', j);
                            });
                            $(document).ready(function () {
                                $("#fm1").submit(function () {
                                    $(":submit").attr("disabled", true);
                                    $(":submit").attr("value", i);
                                    return true;
                                });
                            });</script>
                </div>
            </div>
        </div>
        <div id="providers" style="display: none;">
            <script>var redirectTo = $( "a[autoRedirect=true]" ).attr("href");
            if (redirectTo != null && redirectTo != undefined) {
                window.location.href = redirectTo;
            }</script>
        </div>

        <script>if ($('#list-providers').length) {
                    $('#providers').addClass("col");
                    $('#loginForm').addClass("col");
                    $('#providers').show();
                }</script>

    </div>
</main>


@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
